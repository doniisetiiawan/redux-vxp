{
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:react/recommended", "eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react"],
  "env": {
    "jest": true,
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {},
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./webpack/webpack.dev.babel.js"
      }
    }
  }
}
